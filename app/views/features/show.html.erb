<div id='ticket'>
	<h2><%= @feature.title %></h2>
	<%= simple_format(@feature.description) %>
</div>

<%= link_to "Edit Feature", edit_release_feature_path(@release, @feature) %>
<%= link_to "Delete Feature", [@release, @feature], :method => :delete,
			:confirm => "Are you sure you want to delete this feature?" %>